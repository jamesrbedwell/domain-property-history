{
  "manifest_version": 3,
  "name": "Domain Property History Enhancer",
  "version": "1.0",
  "description": "Adds property history to Domain.com.au listing pages",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["https://www.domain.com.au/*"],
  "content_scripts": [
    {
      "matches": ["https://www.domain.com.au/*"],
      "js": ["content.js"]
    }
  ]
}
